    "version": "0.2.0",
    "configurations": [

{
    "type": "chrome",
    "request": "attach",
    "name": "Attach to Chrome",
    "port": 9222,
    "webRoot": "${workspaceFolder}"
},
        {
            "type": "chrome",
            "request": "launch",
            "name": "React Debugger",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/client/src"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Node",
            "processId": "${command:PickProcess}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Not node",
            "program": "${workspaceFolder}\\index.js"
        }
    ]
}